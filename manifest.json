{
	"manifest_version": 3,
	"name": "WheelCursor",
	"description": "WheelCursor",
	"version": "1.0",
	"action": {
		"default_popup": "popup.html"
	},
	"permissions": ["storage"],
	"host_permissions": ["<all_urls>"],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["dist/contents-script.js"]
		}
	]
}
